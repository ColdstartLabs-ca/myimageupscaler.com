{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "myimageupscaler",
  "main": ".open-next/worker.js",
  "compatibility_date": "2024-12-30",
  "compatibility_flags": ["nodejs_compat"],
  "routes": [
    { "pattern": "myimageupscaler.com/*", "zone_name": "myimageupscaler.com" },
    { "pattern": "www.myimageupscaler.com/*", "zone_name": "myimageupscaler.com" }
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": ".open-next/assets"
  },
  "images": {
    "binding": "IMAGES"
  },
  "observability": {
    "enabled": true
  },
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      "service": "myimageupscaler"
    }
  ]
}
